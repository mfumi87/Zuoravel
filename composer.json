{
    "name": "mfumi87/zuoravel",
    "description": "A Zuora integration package for Laravel",
    "type": "library",
    "keywords": [
        "zuora"
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.6",
        "guzzlehttp/guzzle": "~6.0",
        "mtownsend/xml-to-array": "~1.0"
    },
    "require-dev": {
      "orchestra/testbench": "~3.6.0",
      "phpunit/phpunit": "~7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Lester",
            "email": "roblesterjr04@gmail.com"
        }
    ],
    "scripts": {
      "phpunit": "phpunit"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Lester\\Zuoravel\\ServiceProvider"
        ]
      }
    },
    "autoload": {
      "psr-4": {
        "Lester\\Zuoravel\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Lester\\Zuoravel\\Tests\\": "tests"
      },
      "files": [
        "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
      ]
    }
}
